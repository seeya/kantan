    });\r\n                temp.push(\r\n                  <td\r\n                    key={`edit_${i}`}\r\n                    onClick={() => {\r\n                      this.props.editHandler(i);\r\n                    }}\r\n                  >\r\n                    üñäÔ∏è\r\n                  </td>\r\n                );\r\n                temp.push(\r\n                  <td\r\n                    key={`delete_${i}`}\r\n                    onClick={() => {\r\n                      this.props.deleteHandler(i);\r\n                    }}\r\n                  >\r\n                    üóëÔ∏è\r\n                  </td>\r\n                );\r\n                return temp;\r\n              })\r\n              .map((td, i) => {\r\n                return <tr key={i}>{td}</tr>;\r\n              })}\r\n          </tbody>\r\n        </Table>\r\n      );\r\n    } else return null;\r\n  };\r\n\r\n  render() {\r\n    return this._renderTable();\r\n  }\r\n}\r\n"},"lineCount":null}},"hash":"3951529aaf9b9430159090a6b3c002a7","cacheData":{"env":{}}}